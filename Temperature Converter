function toFahrenheit(temp){
    let fahr=temp *9/5 +32;
    console.log(`${temp} degrees in C is ${fahr} degrees in fahrenheit`);
}
toFahrenheit(40);
function toCelsius(temp){
    let cel=((temp-32)*5/9).toFixed(2);
    console.log(`${temp} degrees in F is ${cel} degrees in Celcius`);
}
toCelsius(100);
